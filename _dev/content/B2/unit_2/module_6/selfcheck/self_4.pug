                        div(class="row mb20")
                            ol(class="c-listDefault")
                                li(class="u-textColor_4")
                                    span(class="u-textColor_1-dark")
                                        span You
                                        span(class="c-textField"): input(type="text" placeholder="Text here..." class="c-input ta-c js-escribir" ng-model="txtcaptura_e")
                                        span be right but I’m going to check anyway.
                            div(class="row-flex")
                                input(class="c-inputRadio" type="radio" id="inputRadio_9" name="SelInputRadio_5" value="1")
                                label(for="inputRadio_9" class="c-CheckSelected col-flex ml10 mr10" ng-click="txtcaptura_e='might'") might

                                input(class="c-inputRadio" type="radio" id="inputRadio_10" name="SelInputRadio_5" value="2")
