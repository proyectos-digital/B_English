                                input(class="c-inputRadio" type="radio" id="inputRadio_4" name="SelInputRadio_2" value="2")
                                label(for="inputRadio_4" class="c-CheckSelected col-flex ml10 mr10" ng-click="txtcaptura_b='can'") can
                        |
                        div(class="row mb20")
                            ol(class="c-listDefault")
                                li(class="u-textColor_4")
                                    span(class="u-textColor_1-dark")
                                        span Look at the time. It’s 10 PM ! Where are the children! They
                                        span(class="c-textField"): input(type="text" placeholder="Text here..." class="c-input ta-c js-escribir" ng-model="txtcaptura_c")
                                        span still be out!
                            div(class="row-flex")
                                input(class="c-inputRadio" type="radio" id="inputRadio_5" name="SelInputRadio_3" value="1")
                                label(for="inputRadio_5" class="c-CheckSelected col-flex ml10 mr10" ng-click="txtcaptura_c='can'") can

                                input(class="c-inputRadio" type="radio" id="inputRadio_6" name="SelInputRadio_3" value="2")
                                label(for="inputRadio_6" class="c-CheckSelected col-flex ml10 mr10" ng-click="txtcaptura_c='can’t'") can’t
                div(class="col-flex-xs12 col-flex-md5 offset-md1 col-center mt10-sm mt20-md")
                        div(class="row mb20")
                            ol(class="c-listDefault")
                                li(class="u-textColor_4")
                                    span(class="u-textColor_1-dark")
                                        span It has been raining a lot! With luck, tomorrow
                                        span(class="c-textField"): input(type="text" placeholder="Text here..." class="c-input ta-c js-escribir" ng-model="txtcaptura_d")
                                        span be a sunny day.
                            div(class="row-flex")
                                input(class="c-inputRadio" type="radio" id="inputRadio_7" name="SelInputRadio_4" value="1")
                                label(for="inputRadio_7" class="c-CheckSelected col-flex ml10 mr10" ng-click="txtcaptura_d='could'") could

                                input(class="c-inputRadio" type="radio" id="inputRadio_8" name="SelInputRadio_4" value="2")
                                label(for="inputRadio_8" class="c-CheckSelected col-flex ml10 mr10" ng-click="txtcaptura_d='can'") can
